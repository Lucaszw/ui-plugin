{
  "name": "@microdrop/ui-plugin",
  "version": "0.1.6",
  "description": "Parent class of microdrop ui plugins",
  "main": "src/ui-plugin.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "./node_modules/.bin/webpack",
    "build-dev": "npm run --prefx ../mqtt-client web && npm run build",
    "dev-mode": "npm uninstall @mqttclient/web && npm i ../mqttclient-web",
    "prod-mode": "npm uninstall @mqttclient/web && npm i @mqttclient/web",
    "update-deps": "ncu -ua @mqttclient/web && npm install",
    "git-add": "git add package-lock.json && git add -p . && git commit",
    "publish-pkg": "npm run prod-mode && npm run build && npm version patch && npm publish && git push origin master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lucaszw/ui-plugin.git"
  },
  "keywords": [
    "Microdrop",
    "UI",
    "Plugin"
  ],
  "author": "Sci-Bots",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/Lucaszw/ui-plugin/issues"
  },
  "homepage": "https://github.com/Lucaszw/ui-plugin#readme",
  "devDependencies": {
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "@mqttclient/web": "^0.1.5",
    "keyboard-shortcut": "^1.2.0"
  }
}
